.profile-info{
  position:relative;
padding:0 20rpx;
height:332rpx;
background:url(http://cdn-new-shanghai.widiazine.com/images/23/2018/12/LXqngM7XoMY373yGO03tNX7zT8w1Y9.jpg) no-repeat;
background-size:100%;
color:#fff;

}
.fui-cell-group .fui-cell{
  padding:10rpx 0;
margin-bottom:20rpx;
font-size: 24rpx;
border-bottom:1px solid #dbdbdb;

}
.fui-cell-info{
  font-size: 24rpx;
}
.fui-cell-group{
  position:absolute;
left:25rpx;
right:25rpx;
top:50rpx;
z-index:10;
padding:20rpx 50rpx;
border-radius:20rpx;
background:#fff;
box-shadow:0 4rpx 20rpx rgba(133,133,133,.15);
font-size:28rpx;
color:#333;

}



.fui-picker {
    height: 300rpx;
    background: red;
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    transform: translate3d(0, 100%, 0);
    transition-property: transform;
}

.fui-picker.in {
    transform: translate3d(0, 0, 0);
    transition-duration: 300ms;
}

.fui-picker.out{
    transition-duration: 300ms;
    transform: translate3d(0, 100%, 0);
}

.fixed {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: #ff3300;
}

